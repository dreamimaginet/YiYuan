@model  YiYuan.ViewModel.ProductViewModel
@{
    var data = Model;
    //var data = ViewBag.Data as ProductViewModel;
    ViewBag.Title = data.Name;
}
@section styles{
    <link href="~/Content/core.css" rel="stylesheet" />
    <link href="~/Content/index.css" rel="stylesheet" />

}
<div class="g-wrap">
    <div class="m-product-nav">
        <a href="../navigationbar/home.html">首页</a>
        <i>&gt;</i>
        <a href="../classbar/all.html">全部商品</a>
        <i>&gt;</i>
        <span>@data.Name</span>
    </div>
    <div class="m-product-info">
        <div class="m-product-img">
            <div class="u-big-img">
                @*<img src="../../image/1445588950504_1.jpg" alt="iPhone6s 64G" height="314">*@

                <img src="@data.ImgUrls.FirstOrDefault()" alt="@data.Name" height="314">
            </div>
            <ul class="m-img-list">
                @foreach (var item in data.ImgUrls)
            {
                    <li class="u-small-img u-small-img0 img-active">
                        <img src="@item" alt="data.Name" height="88">
                    </li>
                }


                @*<li class="u-small-img u-small-img0 img-active">
                        <img src="../../image/1445588950504_1.jpg" alt="iPhone6s 64G" height="88">
                    </li>

                    <li class="u-small-img u-small-img1">
                        <img src="../../image/1445588950504_1.jpg" alt="iPhone6s 64G" height="88">
                    </li>
                    <li class="u-small-img u-small-img2">
                        <img src="../../image/1445588950504_1.jpg" alt="iPhone6s 64G" height="88">
                    </li>
                    <li class="u-small-img u-small-img3">
                        <img src="../../image/1445588950504_1.jpg" alt="iPhone6s 64G" height="88">
                    </li>*@
            </ul>
            <div class="m-promise">
                <div class="m-promise-item">
                    <i class="fair"></i>
                    <span>公正公开</span>
                </div>
                <div class="m-promise-item">
                    <i class="quality"></i>
                    <span>正品保证</span>
                </div>
                <div class="m-promise-item">
                    <i class="guard"></i>
                    <span>权益保障</span>
                </div>
                <div class="m-promise-item">
                    <i class="free"></i>
                    <span>免费配送</span>
                </div>
            </div>
        </div>
        <div class="m-product-detail">
            <div class="m-product-title">
                <h1 title="@data.Name">@data.Name</h1>
                <div class="m-product-desc">
                    @data.Title
                </div>
            </div>
            <div class="m-period">
                <div class="m-period-progress">
                    <div class="m-period-num">第&nbsp;@data.EndNumber&nbsp;期</div>
                    <div class="m-period-bar">
                        <div class="total-person-time">
                            总需人次：@data.EndNumber
                            <span>每满@(data.EndNumber)人次，即抽取1人获得该商品</span>
                        </div>
                        <div class="m-progress-bar">
                            <div class="bar-wrapper">
                                <div class="p-bar" style="width:55%;"></div>
                            </div>
                        </div>
                        <div class="other-time">
                            <div class="joined-time">已参与人次&nbsp;3374</div>
                            <div class="remain-time">剩余人次&nbsp;<span>2706</span></div>
                        </div>
                    </div>
                </div>
                <div class="buy-now">
                    参与
                    <div class="cal-time">
                        <a href="javascript:;" class="minus-time disabled" hidefocus="true">-</a>
                        <input type="text" class="buy-time" value="1" data-min="1" data-max="2706">
                        <a href="javascript:;" class="plus-time" hidefocus="true">+</a>
                    </div>
                    人次
                    <span class="buy-tip">
                        <i class="grey-arrow"></i>
                        参与人次越多，获得概率越大！
                    </span>
                    <a href="javascript:;" class="buy-btn">
                        立即参与
                    </a>
                </div>
                <div class="not-login">
                    请<a href="javascript:;" class="u-login-dialog" hidefocus="true">登录</a>，查看您的参与号码
                </div>
            </div>
        </div>
    </div>
    <div class="p-clearfix"></div>
    <div class="m-tab-list">
        <div class="m-tab-title" data-toggle="bindTab" data-bindtab-method="click" data-bindtab-activecss="tab-active" data-bindtab-itemtag=".tab-item" style="position: static; top: 0px; z-index: 90;">
